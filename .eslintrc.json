{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "amd": true
    },
    "extends": [
        "eslint:recommended",
        "angular"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": "off",
        "quotes": [
            "error",
            "single"
        ],
        "semi": "off"
    }
}
