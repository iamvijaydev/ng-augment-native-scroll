{
    "env": {
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "angular"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": "off",
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}
